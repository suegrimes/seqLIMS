<h1>Sequencing Libraries for Flow Cell</h1>

<%= form_tag :action => 'new', :controller => 'flow_cells' do %>
<table class="list">
  <tr>
    <th>Library Name</th>
	<th>Owner</th>
	<th>Preparation Date</th>
	<th>Source DNA/RNA</th>
	<th>Run Type/Adapter</th>
	<th>Project</th>
	<th>Alignment Ref</th>
  </tr>

<% for seq_lib in @seq_libs %>
  <tr>
    <td><%= link_to seq_lib.lib_name, seq_lib %></td>
	<td><%=h seq_lib.owner %></td>
	<td><%=h seq_lib.preparation_date %></td>
	<td></td>
	<td><%=h seq_lib.runtype_adapter %></td>
	<td><%=h seq_lib.project %></td>
	<td><%=h seq_lib.alignment_ref %></td>
  </tr>
  <% end %>
</table>
<% end %>


